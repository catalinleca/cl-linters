{
  "name": "cl-tools",
  "private": true,
  "version": "1.0.0",
  "description": "My personal eslint packages",
  "main": "index.js",
  "repository": "https://gitlab.com/catalinleca95/cl-tools.git",
  "author": "Catalin Leca <leca.catalin1995@gmail.com>",
  "license": "MIT",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./",
    "lint-fix": "eslint --ext .js,.jsx,.ts,.tsx ./ --fix",
    "prettier": "prettier --check **/*.{tsx,ts,js,jsx,json,css,scss,md,html}",
    "prettier-fix": "prettier --write **/*.{tsx,ts,js,jsx,json,css,scss,md,html}"
  },
  "devDependencies": {
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "lerna": "^4.0.0",
    "lint-staged": "^9.4.1",
    "prettier": "^2.1.2"
  }
}
